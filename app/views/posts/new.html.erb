<%= simple_form_for @post do |f| %>
  <%= f.input :image %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :number_of_seats %>
  <%= f.input :contact_info %>
  <%= f.input :category %>
  <%= f.input :will_start_at %>
  <%= f.input :will_end_at %>
  <%= f.button :submit %>
<% end %>
